DEFINE PROCEDURE ''PRIME-BEYOND'' [N]:
BLOCK 0: BEGIN
	CELL(0) ⇐ 1;
	CELL(1) ⇐ 1;
	LOOP N TIMES:
	BLOCK 1: BEGIN
		IF PRIME? [CELL(1)], THEN:
		CELL(0) ⇐ CELL(0) × CELL(1);
		CELL(1) ⇐ CELL(1) + 1;
	BLOCK 1: END;
	CELL(0) ⇐ CELL(0) + 1;
	OUTPUT ⇐ N;
	LOOP AT MOST CELL(0) TIMES:
	BLOCK 2: BEGIN
		OUTPUT ⇐ OUTPUT + 1;
		IF PRIME? [OUTPUT], THEN:
		ABORT LOOP 2;
	BLOCK 2: END;
BLOCK 0: END.
