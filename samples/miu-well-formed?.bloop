DEFINE PROCEDURE ''MIU-WELL-FORMED?'' [N]:
BLOCK 0: BEGIN
	CELL(0) ⇐ N;
	LOOP AT MOST N TIMES:
	BLOCK 1: BEGIN
		IF CELL(0) = 0, THEN:
		ABORT LOOP 1;
		CELL(1) ⇐ REMAINDER [CELL(0),10];
		IF CELL(1) = 2, THEN:
		QUIT BLOCK 0;
		IF CELL(1) > 3, THEN:
		QUIT BLOCK 0;
		CELL(0) ⇐ DIVIDE [CELL(0),10];
	BLOCK 1: END;
	OUTPUT ⇐ YES;
BLOCK 0: END.
